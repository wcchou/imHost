{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^client-module\\.json$"],
      "matchStrings": ["\"(?<packageName>[\\w]+)\":[\\s\\w\\{]+\"version\":[\\s\"]+(?<currentValue>\\d+.\\d+.\\d+.\\d+)\",\\s+\"path\":\\s+\"(?<depName>[\\w\\/]+)\""],
      "packageNameTemplate": "https://github.com/{{{depName}}}.git",
      "datasourceTemplate": "github-tags",
      "extractVersionTemplate": "nuget"
    }
  ],
  "packageRules": [
    {
      "matchPackagePatterns": ["[\\w\\/]+imLib$"],
      "versioning": "nuget"
    }
  ]
}
