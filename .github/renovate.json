{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^client-module\\.json$"],
      "matchStrings": ["\"(?<packageName>[\\w]+)\":[\\s\\w\\{]+\"version\":[\\s\"]+(?<currentValue>\\d+.\\d+.\\d+.\\d+)\",\\s+\"path\":\\s+\"(?<depName>\\w+)\""],
      "datasourceTemplate": "github-tags",
      "extractVersionTemplate": "nuget",
      "depNameTemplate": "https://github.com/wcchou/{{{depName}}}.git"
    }
  ],
  "packageRules": [
    {
      "matchPackageNames": ["imLib"],
      "versioning": "regex:^(?<major>\\d+)(\\.(?<minor>\\d+))(\\.(?<patch>\\d+))(\\.(?<build>\\d+))$"
    }
  ]
}
